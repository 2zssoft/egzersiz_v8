<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hakkinda"
    tools:listitem="@android:layout/simple_list_item_activated_1" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center|clip_vertical|clip_horizontal"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/textView_id_yazisi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Developer"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView_aciklama"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/musa_selman_kundurac_"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="38dp"
                android:layout_height="match_parent"
                android:src="@drawable/twitter" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="mskunduraci" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/email" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="mskunduraci@gmail.com" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView4_dil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/turkatletizm_websitesi"
            android:textAppearance="?android:attr/textAppearanceMedium" />

    </LinearLayout>

</LinearLayout>